{
  "name": "predictions",
  "version": "2.0.0",
  "description": "Predictions, built with Pyth Oracle and In-House Oracle",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "npx hardhat compile",
    "deployFactory:testnet": "npx hardhat run --network testnet scripts/deployFactory.ts",
    "deployFactory:mainnet": "npx hardhat run --network mainnet scripts/deployFactory.ts",
    "deployPredictionV2:testnet": "npx hardhat run --network testnet scripts/deployPredictionV2.ts",
    "deployPredictionV2:mainnet": "npx hardhat run --network mainnet scripts/deployPredictionV2.ts",
    "deployPredictionV3:testnet": "npx hardhat run --network testnet scripts/deployPredictionV3.ts",
    "deployPredictionV3:mainnet": "npx hardhat run --network mainnet scripts/deployPredictionV3.ts",
    "deployPredictionV4:testnet": "npx hardhat run --network testnet scripts/deployPredictionV4.ts",
    "deployPredictionV4:mainnet": "npx hardhat run --network mainnet scripts/deployPredictionV4.ts",
    "deployPredictionV5:testnet": "npx hardhat run --network testnet scripts/deployPredictionV5.ts",
    "deployPredictionV5:mainnet": "npx hardhat run --network mainnet scripts/deployPredictionV5.ts",
    "test": "npx hardhat test",
    "test:coverage": "npx hardhat coverage",
    "test:size-contracts": "npx hardhat size-contracts",
    "lint": "solhint 'contracts/**/*.sol'",
    "format:check": "prettier --check './**/*.{js,ts,sol}'",
    "format:write": "prettier --write './**/*.{js,ts,sol}'",
    "clean": "rimraf ./cache ./artifacts ./node_modules",
    "postinstall": "yarn husky install && cp ./gitmojis.json ./node_modules/commitlint-plugin-gitmoji/lib/gitmojis.json",
    "hook:prepare-commit-msg": "node githooks/prepare-commit-msg.ts"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.2.2",
    "@nomiclabs/hardhat-web3": "2.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "@openzeppelin/hardhat-upgrades": "1.22.1",
    "@openzeppelin/test-helpers": "^0.5.11",
    "ethereum-waffle": "3.4.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "^14.14.35",
    "chai": "4.3.7",
    "dotenv": "^8.2.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.22.9",
    "pre-commit": "^1.2.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "commitlint-config-gitmoji": "^2.2.5",
    "commitlint-plugin-gitmoji": "^2.2.0",
    "husky": "^8.0.3",
    "prettier": "3.2.4",
    "prettier-plugin-solidity": "1.3.1",
    "solhint": "4.1.1",
    "solidity-coverage": "^0.8.13",
    "hardhat-contract-sizer": "2.8.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@pythnetwork/pyth-sdk-solidity": "^4.0.0"
  }
}
